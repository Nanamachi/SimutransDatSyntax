            backDiagonal[SW]=${1:image}.Y.X
            $0
        """
    #
    # diagonal image
    'WAY:IMG:backDiagonal':
        'prefix': 'diagonal'
        'body': """
            diagonal[NE]=${1:image}.${2:Y}.${3:X}
            diagonal[NW]=${1:image}.${4:Y}.${5:X}
            diagonal[SE]=${1:image}.${6:Y}.${7:X}
            diagonal[SW]=${1:image}.${8:Y}.${9:X}
        """
